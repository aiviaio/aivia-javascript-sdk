{
  "extends": ["eslint:all", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jest": true
  },
  "globals": {
    "artifacts": false,
    "contract": false,
    "assert": false,
    "web3": false
  },
  "rules": {
    "no-magic-numbers": 0,
    "no-console": 0,
    "one-var": 0,
    "camelcase": 0,
    "capitalized-comments": 0,
    "multiline-comment-style": 0
  }
}
